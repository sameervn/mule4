  <xs:element xmlns="http://www.mulesoft.org/schema/mule/mongo" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="InsertDocumentsType" substitutionGroup="mule:abstract-operator" name="insert-documents">
    <xs:annotation>
      <xs:documentation>Insert a list of documents in a collection. The _id object can be specified or not (and mongo will auto generate one).</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="InsertDocumentsType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractOperatorType">
        <xs:sequence minOccurs="0" maxOccurs="1">
          <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstract-reconnection-strategy"></xs:element>
          <xs:element type="xs:string" minOccurs="0" maxOccurs="1" name="documents">
