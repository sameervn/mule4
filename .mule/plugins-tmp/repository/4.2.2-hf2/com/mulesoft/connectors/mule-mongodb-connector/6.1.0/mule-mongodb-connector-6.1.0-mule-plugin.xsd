<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:mule="http://www.mulesoft.org/schema/mule/core" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.mulesoft.org/schema/mule/mongo" attributeFormDefault="unqualified" elementFormDefault="qualified">
  <xs:import namespace="http://www.w3.org/XML/1998/namespace"></xs:import>
  <xs:import namespace="http://www.springframework.org/schema/beans" schemaLocation="http://www.springframework.org/schema/beans/spring-beans.xsd"></xs:import>
  <xs:import namespace="http://www.mulesoft.org/schema/mule/core" schemaLocation="http://www.mulesoft.org/schema/mule/core/current/mule.xsd"></xs:import>
  <xs:import namespace="http://www.mulesoft.org/schema/mule/tls" schemaLocation="http://www.mulesoft.org/schema/mule/tls/current/mule-tls.xsd"></xs:import>
  <xs:element xmlns:mule="http://www.mulesoft.org/schema/mule/core" substitutionGroup="mule:abstract-shared-extension" name="config">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="mule:abstractExtensionType">
          <xs:sequence minOccurs="1" maxOccurs="1">
            <xs:element minOccurs="1" maxOccurs="1" ref="mule:abstractConnectionProvider"></xs:element>
            <xs:element minOccurs="0" maxOccurs="1" ref="mule:expiration-policy"></xs:element>
            <xs:element minOccurs="0" maxOccurs="1" name="read-preference-tags">
              <xs:annotation>
                <xs:documentation>If a replica set member or members are associated with tags, you can specify a tag set (array of tag specification documents) in the read preference to target those members.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element xmlns:mongo="http://www.mulesoft.org/schema/mule/mongo" minOccurs="0" maxOccurs="unbounded" ref="mongo:abstract-read-preference-tag"></xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
          <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/mongo" type="com.mulesoft.connector.mongo.api.ReadConcernOptionsEnumType" use="optional" name="readConcern">
            <xs:annotation>
              <xs:documentation>The read concern option allows you to control the consistency and isolation properties of the data read from replica sets and replica set shards.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/mongo" type="com.mulesoft.connector.mongo.api.ReadPreferenceEnumType" use="optional" default="PRIMARY" name="readPreference">
            <xs:annotation>
              <xs:documentation>Specifies the read preferences for this connection.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="mule:substitutableInt" use="optional" default="-1" name="maximumStalenessSeconds">
            <xs:annotation>
              <xs:documentation>Specifies, in seconds, how stale a secondary can be before the client stops using it for read operations.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="mule:substitutableName" use="optional" name="readPreferenceTags">
            <xs:annotation>
              <xs:documentation>If a replica set member or members are associated with tags, you can specify a tag set (array of tag specification documents) in the read preference to target those members.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="required" name="name">
            <xs:annotation>
              <xs:documentation>The identifier of this element used to reference it in other components</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="optional" name="writeConcernAcknowledgement">
            <xs:annotation>
              <xs:documentation>Requests acknowledgment that the write operation has propagated to the specified number of mongod instances. You can write a number of instances n&gt;0 or use a value from the list.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="mule:substitutableInt" use="optional" name="writeConcernTimeout">
            <xs:annotation>
              <xs:documentation>This option specifies a time limit, in milliseconds, for the write concern. it is only applicable for Write Concern Acknowledgement values greater than 1</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/mongo" type="java.util.concurrent.TimeUnitEnumType" use="optional" default="SECONDS" name="writeConcernTimeoutTimeunit">
            <xs:annotation>
              <xs:documentation>The Timeunit for the Write Concern Timeout.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="com.mulesoft.connector.mongo.api.ReadPreferenceTag">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractExtensionType">
        <xs:sequence minOccurs="0" maxOccurs="1">
          <xs:element minOccurs="0" maxOccurs="1" name="name-values">
            <xs:complexType>
              <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" name="name-value">
                  <xs:complexType>
                    <xs:attribute type="mule:expressionString" use="required" name="key"></xs:attribute>
                    <xs:attribute type="mule:expressionString" use="required" name="value"></xs:attribute>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:attribute type="mule:expressionMap" use="optional" name="nameValue"></xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns:mongo="http://www.mulesoft.org/schema/mule/mongo" type="mongo:com.mulesoft.connector.mongo.api.ReadPreferenceTag" abstract="true" name="abstract-read-preference-tag"></xs:element>
  <xs:element xmlns:mongo="http://www.mulesoft.org/schema/mule/mongo" substitutionGroup="mongo:abstract-read-preference-tag" name="read-preference-tag">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="mongo:com.mulesoft.connector.mongo.api.ReadPreferenceTag"></xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element xmlns:mule="http://www.mulesoft.org/schema/mule/core" substitutionGroup="mule:abstractConnectionProvider" name="connection">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="mule:abstractConnectionProviderType">
          <xs:sequence minOccurs="0" maxOccurs="1">
            <xs:element minOccurs="0" maxOccurs="1" ref="mule:reconnection"></xs:element>
            <xs:element xmlns:tls="http://www.mulesoft.org/schema/mule/tls" minOccurs="0" maxOccurs="1" ref="tls:context"></xs:element>
            <xs:element minOccurs="0" maxOccurs="1" name="server-addresses">
              <xs:annotation>
                <xs:documentation>The server where your MongoDB instance is. You can set multiple servers.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element xmlns:mongo="http://www.mulesoft.org/schema/mule/mongo" minOccurs="0" maxOccurs="unbounded" ref="mongo:abstract-server-address"></xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="1" name="compressors">
              <xs:annotation>
                <xs:documentation>List of compressors to enable network compression for communication between this client and a mongod/mongos instance. If you specify multiple compressors, then the order in which you list the compressors matter as well as the communication initiator.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element minOccurs="0" maxOccurs="unbounded" name="compressor">
                    <xs:complexType>
                      <xs:attribute type="mule:expressionString" use="required" name="value"></xs:attribute>
                    </xs:complexType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="1" name="pooling-settings">
              <xs:complexType>
                <xs:complexContent>
                  <xs:extension base="mule:abstractExtensionType">
                    <xs:sequence></xs:sequence>
                    <xs:attribute type="mule:substitutableInt" use="optional" name="minConnectionPoolSize">
                      <xs:annotation>
                        <xs:documentation>The minimum size for the connection pool.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute type="mule:substitutableInt" use="optional" default="100" name="maxConnectionPoolSize">
                      <xs:annotation>
                        <xs:documentation>The maximum size for the connection pool.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute type="mule:substitutableInt" use="optional" default="120" name="maxWaitQueueTime">
                      <xs:annotation>
                        <xs:documentation>The maximum wait queue time for the connection pool.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/mongo" type="java.util.concurrent.TimeUnitEnumType" use="optional" default="SECONDS" name="maxWaitQueueTimeUnit">
                      <xs:annotation>
                        <xs:documentation>The maximum wait queue time unit for the connection pool.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute type="mule:substitutableInt" use="optional" name="maxConnectionLifeTime">
                      <xs:annotation>
                        <xs:documentation>The maximum connection lifetime for the connection pool.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/mongo" type="java.util.concurrent.TimeUnitEnumType" use="optional" default="SECONDS" name="maxConnectionLifeTimeUnit">
                      <xs:annotation>
                        <xs:documentation>The maximum connection lifetime Time Unit for the connection pool.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute type="mule:substitutableInt" use="optional" name="maxConnectionIdleTime">
                      <xs:annotation>
                        <xs:documentation>The maximum connection idle time for the connection pool.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/mongo" type="java.util.concurrent.TimeUnitEnumType" use="optional" default="SECONDS" name="maxConnectionIdleTimeUnit">
                      <xs:annotation>
                        <xs:documentation>The maximum connection idle time Time Unit for the connection pool.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                  </xs:extension>
                </xs:complexContent>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
          <xs:attribute type="xs:string" use="optional" name="tlsContext"></xs:attribute>
          <xs:attribute type="mule:expressionList" use="optional" name="serverAddresses">
            <xs:annotation>
              <xs:documentation>The server where your MongoDB instance is. You can set multiple servers.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="mule:expressionString" use="required" name="database">
            <xs:annotation>
              <xs:documentation>Specify the database name that you want to connecto to.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="mule:expressionString" use="optional" name="username">
            <xs:annotation>
              <xs:documentation>The username to use for authentication.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="mule:expressionString" use="optional" name="password">
            <xs:annotation>
              <xs:documentation>The password to use for authentication. If the password is null or whitespaces only, the connector won't use authentication and username must be empty too.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="mule:expressionString" use="optional" name="replicaSetName">
            <xs:annotation>
              <xs:documentation>The name of the replica set to connect to. The server Addresses list can be partial when using this parameter. This means that the user can set just one host on Server Addresses, but set this value and It will connect to the full cluster</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="mule:expressionString" use="optional" name="authenticationSource"></xs:attribute>
          <xs:attribute type="mule:substitutableName" use="optional" name="compressors">
            <xs:annotation>
              <xs:documentation>List of compressors to enable network compression for communication between this client and a mongod/mongos instance. If you specify multiple compressors, then the order in which you list the compressors matter as well as the communication initiator.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="mule:substitutableInt" use="optional" default="-1" name="zlibCompressionLevel">
            <xs:annotation>
              <xs:documentation>An integer that specifies the compression level if using zlib for network compression. 0 means No Compression, 1-9 are the compression levels, with 1 being the lowest compression level, and 9 the most compressions (and more processing time)</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="mule:substitutableInt" use="optional" default="30" name="connectionTimeout">
            <xs:annotation>
              <xs:documentation>The connection timeout; this is for establishing the socket connections (open). 0 is infinite.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/mongo" type="java.util.concurrent.TimeUnitEnumType" use="optional" default="SECONDS" name="connectionTimeoutTimeunit">
            <xs:annotation>
              <xs:documentation>The connection timeout unit.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="mule:substitutableInt" use="optional" default="15" name="localThreshold">
            <xs:annotation>
              <xs:documentation>The size of the latency window for selecting among multiple suitable MongoDB instances.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/mongo" type="java.util.concurrent.TimeUnitEnumType" use="optional" default="MILLISECONDS" name="localThresholdTimeUnit">
            <xs:annotation>
              <xs:documentation>The local threshold Time Unit.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="mule:substitutableInt" use="optional" default="30" name="serverSelectionTimeout">
            <xs:annotation>
              <xs:documentation>Specifies how long to block for server selection before throwing an exception.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/mongo" type="java.util.concurrent.TimeUnitEnumType" use="optional" default="SECONDS" name="serverSelectionTimeoutTimeUnit">
            <xs:annotation>
              <xs:documentation>The server selection Time Unit.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="mule:substitutableInt" use="optional" name="socketTimeout">
            <xs:annotation>
              <xs:documentation>The socket timeout. It is used for I/O socket read and write operations. 0 is default and infinite.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/mongo" type="java.util.concurrent.TimeUnitEnumType" use="optional" default="SECONDS" name="socketTimeoutUnit">
            <xs:annotation>
              <xs:documentation>The socket timeout unit.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="mule:substitutableBoolean" use="optional" default="false" name="retryWrites">
            <xs:annotation>
              <xs:documentation>Enable retryable writes</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/mongo" type="com.mulesoft.connector.mongo.api.AuthenticationMechanismEnumType" use="optional" name="authenticationMechanism">
            <xs:annotation>
              <xs:documentation>The authentication Mechanism used for this connection</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="com.mulesoft.connector.mongo.api.ServerAddress">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractExtensionType">
        <xs:attribute type="mule:expressionString" use="required" name="host"></xs:attribute>
        <xs:attribute type="mule:expressionInt" use="optional" default="27017" name="port"></xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns:mongo="http://www.mulesoft.org/schema/mule/mongo" type="mongo:com.mulesoft.connector.mongo.api.ServerAddress" abstract="true" name="abstract-server-address"></xs:element>
  <xs:element xmlns:mongo="http://www.mulesoft.org/schema/mule/mongo" substitutionGroup="mongo:abstract-server-address" name="server-address">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="mongo:com.mulesoft.connector.mongo.api.ServerAddress"></xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/mongo" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="Object-listenerType" substitutionGroup="mule:abstract-message-source" name="object-listener"></xs:element>
  <xs:complexType name="Object-listenerType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageSourceType">
        <xs:sequence minOccurs="1" maxOccurs="1">
          <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstract-redelivery-policy"></xs:element>
          <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstract-byte-streaming-strategy"></xs:element>
          <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstract-reconnection-strategy"></xs:element>
          <xs:element minOccurs="1" maxOccurs="1" ref="mule:scheduling-strategy"></xs:element>
        </xs:sequence>
        <xs:attribute type="xs:string" use="required" name="collectionName">
          <xs:annotation>
            <xs:documentation>The collection that will be watched to detect the matching documents.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="watermarkField">
          <xs:annotation>
            <xs:documentation>The field that will queried to detect the objects that match the trigger condition.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>The name of the configuration to be used to execute this component</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:substitutableBoolean" use="optional" name="primaryNodeOnly">
          <xs:annotation>
            <xs:documentation>Whether this source should only be executed on the primary node when runnning in Cluster</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/mongo" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="AddUserType" substitutionGroup="mule:abstract-operator" name="add-user">
    <xs:annotation>
      <xs:documentation>Adds a new user for this db</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="AddUserType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractOperatorType">
        <xs:sequence minOccurs="0" maxOccurs="1">
          <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstract-reconnection-strategy"></xs:element>
        </xs:sequence>
        <xs:attribute type="mule:expressionString" use="required" name="username">
          <xs:annotation>
            <xs:documentation>Name of the user</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="required" name="password">
          <xs:annotation>
            <xs:documentation>Password that will be used for authentication</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>The name of the configuration to be used to execute this component</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/mongo" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="CollectionExistsType" substitutionGroup="mule:abstract-operator" name="collection-exists">
    <xs:annotation>
      <xs:documentation>Answers if a collection exists given its name</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="CollectionExistsType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractOperatorType">
        <xs:sequence minOccurs="0" maxOccurs="1">
          <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstract-reconnection-strategy"></xs:element>
        </xs:sequence>
        <xs:attribute type="mule:expressionString" use="required" name="collectionName">
          <xs:annotation>
            <xs:documentation>the name of the collection</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>The name of the configuration to be used to execute this component</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="target">
          <xs:annotation>
            <xs:documentation>The name of a variable on which the operation's output will be placed</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="optional" default="#[payload]" name="targetValue">
          <xs:annotation>
            <xs:documentation>An expression that will be evaluated against the operation's output and the outcome of that expression will be stored in the target variable</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/mongo" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="CountDocumentsType" substitutionGroup="mule:abstract-operator" name="count-documents">
    <xs:annotation>
      <xs:documentation>Counts the number of documents that match the given query. If no query is passed, returns the number of elements in the collection</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="CountDocumentsType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractOperatorType">
        <xs:sequence minOccurs="0" maxOccurs="1">
          <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstract-reconnection-strategy"></xs:element>
          <xs:element type="xs:string" minOccurs="0" maxOccurs="1" name="query">
            <xs:annotation>
              <xs:documentation>The optional query for counting documents. Only documents matching it will be counted. If unspecified, all documents are counted. It can be: {"field1": "value1"} Or contain operators: { "field1": { $gte: 1, $lt:  10 } }</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:attribute type="mule:expressionString" use="required" name="collectionName">
          <xs:annotation>
            <xs:documentation>The target collection</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="optional" name="writeConcernAcknowledgement"></xs:attribute>
        <xs:attribute type="mule:expressionInt" use="optional" name="writeConcernTimeout"></xs:attribute>
        <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/mongo" type="java.util.concurrent.TimeUnitEnumType" use="optional" name="writeConcernTimeoutTimeunit"></xs:attribute>
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>The name of the configuration to be used to execute this component</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="target">
          <xs:annotation>
            <xs:documentation>The name of a variable on which the operation's output will be placed</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="optional" default="#[payload]" name="targetValue">
          <xs:annotation>
            <xs:documentation>An expression that will be evaluated against the operation's output and the outcome of that expression will be stored in the target variable</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/mongo" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="CreateCollectionType" substitutionGroup="mule:abstract-operator" name="create-collection">
    <xs:annotation>
      <xs:documentation>Creates a new collection. If the collection already exists, a MongoException will be thrown.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="CreateCollectionType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractOperatorType">
        <xs:sequence minOccurs="0" maxOccurs="1">
          <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstract-reconnection-strategy"></xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="capped-options">
            <xs:complexType>
              <xs:complexContent>
                <xs:extension base="mule:abstractExtensionType">
                  <xs:sequence></xs:sequence>
